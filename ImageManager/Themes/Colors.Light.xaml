<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="WindowBackgroundBrush" Color="#F5F5F5"/>
    <SolidColorBrush x:Key="PanelBackgroundBrush"   Color="White"/>
    <SolidColorBrush x:Key="BorderBrushBase"        Color="#D0D0D0"/>
    <SolidColorBrush x:Key="ForegroundBrush"        Color="#1A1A1A"/>
    <SolidColorBrush x:Key="AccentBrush"            Color="#007ACC"/>
    <SolidColorBrush x:Key="AccentBrushPressed"     Color="#005A9E"/>
    <SolidColorBrush x:Key="ButtonBgBrush"          Color="#007ACC"/>
    <SolidColorBrush x:Key="ButtonFgBrush"          Color="White"/>
    <SolidColorBrush x:Key="ChipBrush"              Color="#EAEAEA"/>
</ResourceDictionary>
